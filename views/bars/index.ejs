<div class="container">
  <h2>Bars</h2>
  <div class="grid-container">
    <div class="row bar-index">
        <% bars.forEach((bar) => { %>


          <div class="card bar-card" style="width: 18rem;">
            <img class="card-img-top bar-thumbnail" src="<%= bar.imageOne %>" alt="Card image cap">
            <div class="card-body barIndex">
              <h5 class="card-title"><%= bar.name %></h5>
              <a href="/bars/<%= bar.id %>" class="btn btn-outline-primary my-2 my-sm-0">View</a>
            </div>
          </div>


        <% }); %>
    </div>
  </div>


</div>
